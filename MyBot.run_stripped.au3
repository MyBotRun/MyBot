Else
SetLog("Dropping Clan Castle", $COLOR_BLUE)
Click(GetXPosOfArmySlot($slot, 68), 595 + $bottomOffsetY, 1, $iDelaydropCC2, "#0090")
If _Sleep($iDelaydropCC1) Then Return
Click($x, $y, 1, 0, "#0091")
