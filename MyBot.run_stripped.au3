Global Enum $eWeakEagle = 1, $eWeakInferno, $eWeakXBow, $eWeakWizard, $eWeakMortar
Global $weakDefenseNames[6] = ["None", "Eagle Artillery", "Inferno Tower", "XBow", "Wizard Tower", "Mortar"]
Global $weakDefenseMaxLevels[6] = [0, 2, 4, 4, 9, 9]
