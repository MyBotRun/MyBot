ControlMove($hGUI_LOG, "", $txtLog, $x, $y, $w, $h + ($h / 2))
$y += $h + ($h / 2) + $iDividerHeight
ControlHide($hGUI_LOG, "", $divider)
ControlShow($hGUI_LOG, "", $txtAtkLog)
ControlMove($hGUI_LOG, "", $txtAtkLog, $x, $y, $w, $h - ($h / 2))
Case 3
