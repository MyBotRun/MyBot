Local $tIcon = DllStructCreate("handle Handle")
Local $iRet
If $bLarge Then
$iRet = _WinAPI_ExtractIconEx($sImageFile, $iIconID, $tIcon, 0, 1)
Else
$iRet = _WinAPI_ExtractIconEx($sImageFile, $iIconID, 0, $tIcon, 1)
