ClickP($aAway, 1, 0, "#0376")
Return
EndIf
$sInfo = BuildingInfo(242, 520 + $bottomOffsetY)
If $sInfo[0] > 1 Or $sInfo[0] = "" Then
If StringInStr($sInfo[1], "clan") = 0 Then
If $sInfo[0] = "" Then
$sLocMsg = "Nothing"
Else
