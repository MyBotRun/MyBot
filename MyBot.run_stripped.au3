EndIf
Else
Switch $Android
Case "BlueStacks", "BlueStacks2"
Case Else
SetLog($Android & " Display Font Scale cannot be verified", $COLOR_RED)
EndSwitch
EndIf
SetError(0, 0, 0)
$ok = Execute("CheckScreen" & $Android & "(" & $bSetLog & ")")
If $ok = "" And @error <> 0 Then Return True
Return $ok
EndFunc
