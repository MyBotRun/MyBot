Local $px = StringSplit($MilkFarmOffsetElixir[$level], "-", 2)
Case "ddrill"
Local $px = StringSplit($MilkFarmOffsetDark[$level], "-", 2)
Case Else
Local $px = StringSplit("0-0", "-", 2)
