If $iFreeBuilderCount > 0 Then
ClickP($aAway, 1, 0, "#0313")
Local $MinWallGold = Number($iGoldCurrent - $WallCost) > Number($itxtWallMinGold)
Local $MinWallElixir = Number($iElixirCurrent - $WallCost) > Number($itxtWallMinElixir)
Switch $iUseStorage
Case 0
