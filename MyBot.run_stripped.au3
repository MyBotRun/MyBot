If $Debugsetlog = 1 Then SetLog(_PadStringCenter("  Display DPI setting = " & $iDPIRatio & "  ", 53, "+"), $COLOR_BLUE)
ConsoleWrite('DPI= ' & $iDPIRatio & @CRLF)
