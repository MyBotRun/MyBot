If _Sleep($iDelayGoldElixirChange1) Then Return
If getGoldVillageSearch(48, 69) = "" And getElixirVillageSearch(48, 69 + 29) = "" Then
SetLog("Battle has finished", $COLOR_GREEN)
ExitLoop
