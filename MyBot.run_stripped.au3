If $debugsetlogTrain = 1 Then SetLog("Train: need to make normal troops: " & $isNormalBuild, $COLOR_PURPLE)
If $isDarkBuild = "" Or $FirstStart Then
For $i = 0 To UBound($TroopDarkName) - 1
